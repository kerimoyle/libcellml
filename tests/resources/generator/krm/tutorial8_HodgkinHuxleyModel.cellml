<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="Tutorial8_HHModel">
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="tutorial7_SodiumChannelModel.cellml">
    <component component_ref="sodiumChannel" name="sodium_channel"/>
  </import>
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="tutorial6_PotassiumChannelModel.cellml">
    <component component_ref="potassiumChannel" name="potassium_channel"/>
  </import>
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="tutorial8_LeakageCurrentModel.cellml">
    <component component_ref="leakageCurrent" name="leakage"/>
  </import>
  <import xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="tutorial8_controller.cellml">
    <component component_ref="parameters" name="parameters"/>
  </import>
  <units name="mV">
    <unit prefix="milli" units="volt"/>
  </units>
  <units name="ms">
    <unit prefix="milli" units="second"/>
  </units>
  <units name="microA_per_cm2">
    <unit prefix="micro" units="ampere"/>
    <unit exponent="-2" prefix="centi" units="metre"/>
  </units>
  <units name="microF_per_cm2">
    <unit prefix="micro" units="farad"/>
    <unit exponent="-2" prefix="centi" units="metre"/>
  </units>
  <component name="membrane">
    <variable name="V" units="mV" interface="public_and_private"/>
    <variable name="t" units="ms" interface="public_and_private"/>
    <variable name="i_K" units="microA_per_cm2" interface="public_and_private"/>
    <variable name="i_Na" units="microA_per_cm2" interface="public_and_private"/>
    <variable name="i_L" units="microA_per_cm2" interface="public_and_private"/>
    <variable name="i_stim" units="microA_per_cm2" interface="public_and_private"/>
    <variable name="i_tot" units="microA_per_cm2"/>
    <variable name="Cm" units="microF_per_cm2" interface="public_and_private"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>i_stim</ci>
        <piecewise>
          <piece>
            <cn cellml:units="microA_per_cm2">0</cn>
            <apply>
              <lt/>
              <ci>t</ci>
              <cn cellml:units="ms">1</cn>
            </apply>
          </piece>
          <piece>
            <cn cellml:units="microA_per_cm2">0</cn>
            <apply>
              <gt/>
              <ci>t</ci>
              <cn cellml:units="ms">1.2</cn>
            </apply>
          </piece>
          <otherwise>
            <cn cellml:units="microA_per_cm2">100</cn>
          </otherwise>
        </piecewise>
      </apply>
      <apply>
        <eq/>
        <ci>i_tot</ci>
        <apply>
          <plus/>
          <ci>i_stim</ci>
          <ci>i_K</ci>
          <ci>i_Na</ci>
          <ci>i_L</ci>
        </apply>
      </apply>
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>t</ci>
          </bvar>
          <ci>V</ci>
        </apply>
        <apply>
          <divide/>
          <apply>
            <times/>
            <cn cellml:units="dimensionless">-1</cn>
            <ci>i_tot</ci>
          </apply>
          <ci>Cm</ci>
        </apply>
      </apply>
    </math>
  </component>
  <connection component_1="membrane" component_2="parameters">
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="Cm" variable_2="Cm"/>
  </connection>
  <connection component_1="membrane" component_2="sodium_channel">
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="t" variable_2="t"/>
    <map_variables variable_1="i_Na" variable_2="i_Na"/>
  </connection>
  <connection component_1="membrane" component_2="potassium_channel">
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="t" variable_2="t"/>
    <map_variables variable_1="i_K" variable_2="i_K"/>
  </connection>
  <connection component_1="membrane" component_2="leakage">
    <map_variables variable_1="V" variable_2="V"/>
    <map_variables variable_1="i_L" variable_2="i_L"/>
  </connection>
  <encapsulation>
    <component_ref component="membrane">
      <component_ref component="sodium_channel"/>
      <component_ref component="potassium_channel"/>
      <component_ref component="leakage"/>
    </component_ref>
  </encapsulation>
</model>
